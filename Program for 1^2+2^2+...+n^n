#include<stdio.h>
#include<math.h>

int main()
{
    int i , result=0 , n;
    printf("A Program for 1^2+2^2+...+n^n !");
    
    printf("\n\nType a value for series n : ");
    scanf("%d",&n);
    
    for(i=1;i<=n;i++)
    {
        result+= pow(i,2);
    }
    
    printf("\n\nYour Series %d And the final series result is : %d ",n,result);
    
    return 0;
}
